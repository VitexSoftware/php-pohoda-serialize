{
    "name": "vitexsoftware/pohodaser",
    "description": "Stormware's Pohoda XML/PHP serialization",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "CyberVitexus",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Pohoda\\": "src/Pohoda/"
        }
    },
    "require": {
        "goetas-webservices/xsd2php-runtime": "^0.2",
        "jms/serializer": "^3.0",
        "symfony/yaml": "^7.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^12.3",
        "goetas-webservices/xsd2php": "^0.4",
        "friendsofphp/php-cs-fixer": "^3.0",
        "ergebnis/composer-normalize": "^2.0",
        "ergebnis/php-cs-fixer-config": "^6.0",
        "rector/rector": "^0.15"
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
